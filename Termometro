class Termometro {
    private double temperaturaCelsius;
    public void setTemperatura(double temp) {
        this.temperaturaCelsius = temp;
    }
    public double getTemperaturaFahrenheit() {
        return (temperaturaCelsius * 1.8) + 32;
    }
}
public class Main {
    public static void main(String[] args) 
        java.util.Scanner sc = new java.util.Scanner(System.in);
        Termometro t = new Termometro();
        System.out.print("Digite la temperatura en Celsius: ");
        double temp = sc.nextDouble();
        t.setTemperatura(temp);
        System.out.println("Temperatura en Fahrenheit: " + t.getTemperaturaFahrenheit());
        sc.close();
    }
}
