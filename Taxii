// Vehiculo.java
public class Vehiculo {
    String matricula;
    String modelo;
    public void arrancarMotor() {
        System.out.println("El motor ha arrancado.");
    }
}
public class Taxi extends Vehiculo {
    String numeroLicencia;
    public static void main(String[] args) {
        Taxi t = new Taxi();
        t.matricula = "ABC-123";
        t.modelo = "Chevrolet Spark";
        t.numeroLicencia = "LIC-5566";
        t.arrancarMotor(); // método heredado
        System.out.println("Matrícula del Taxi: " + t.matricula);
        System.out.println("Modelo del vehiculo: " + t.modelo);
        System.out.println("Número de Licencia: " + t.numeroLicencia);
    }
}
